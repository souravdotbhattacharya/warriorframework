<?xml version="1.0" ?>
<Testcase>
	<Details>
		<Name>connect_abort_as_fail</Name>
		<Title>onError abort as failure</Title>
		<InputDataFile>../../../data/cli_tests/connect_ssh_Data.xml</InputDataFile>
		<Datatype>Custom</Datatype>
		<default_onError action="next"/>
		<Logsdir>../../../Execution</Logsdir>
		<Resultsdir>../../../Execution</Resultsdir>
		<Engineer>vsethura</Engineer>
	</Details>
	<Steps>
		<step Driver="cli_driver" Keyword="connect" TS="1">
			<Arguments>
				<argument name="system_name" value="rtxttalp01_invalid_port"/>
				<argument name="int_timeout" value="50"/>
				<argument name="prompt" value="labadmin@rtxttalp01:"/>
			</Arguments>
			<onError action="abort_as_error"/>
			<Description>connect</Description>
			<Execute ExecType="Yes"/>
			<context>positive</context>
			<impact>impact</impact>
		</step>
		<step Driver="cli_driver" Keyword="disconnect" TS="2">
			<Arguments>
				<argument name="system_name" value="rtxttalp01_invalid_port"/>
			</Arguments>
			<onError action="next"/>
			<Description>disconnect</Description>
			<Execute ExecType="Yes"/>
			<context>positive</context>
			<impact>impact</impact>
		</step>
	</Steps>
</Testcase>
