<Testcase>

    <Details>
        <Name>connect_telnet_custom_keystroke</Name>
        <Title>Testcase to send custom keystroke when initial connect timeout occur</Title>
        <InputDataFile>../../../data/cli_tests/connect_telnet_custom_keystroke.xml</InputDataFile>
        <Datatype>custom</Datatype>
        <default_onError action="next"/>
        <Logsdir>../../../Execution</Logsdir>
        <Resultsdir>../../../Execution</Resultsdir>
    </Details>

    <Requirements>
        <requirement>ssh-iterative-001</requirement>
    </Requirements>

    <Steps>
        <step Driver="cli_driver" Keyword="connect">
        <Arguments>
            <argument name="system_name" value="ne1_tl1" />
        </Arguments>
        <onError action='next' value='4' />
        <context>positive</context>
        </step>

        <step Driver="cli_driver" Keyword="connect">
        <Arguments>
            <argument name="system_name" value="cds" />
        </Arguments>
        <onError action='next' value='4' />
        <context>positive</context>
        </step>

        <step Driver="cli_driver" Keyword="send_commands_by_testdata_title" >
        <Arguments>
            <argument name="system_name" value="ne1_tl1" />
            <argument name="title" value="login" />
            <argument name="description" value="my own description" />
        </Arguments>
        <onError action='next' value='4'  />
        <rmt></rmt>
        <context>positive</context>
        </step>

        <step Driver="cli_driver" Keyword="send_commands_by_testdata_title" >
        <Arguments>
            <argument name="system_name" value="ne1_tl1" />
            <argument name="title" value="logout" />
            <argument name="description" value="my own description" />
        </Arguments>
        <onError action='next' value='4'  />
        <rmt></rmt>
            <impact>noimpact</impact>
        <context>positive</context>
        </step>

        <step Driver="cli_driver" Keyword="send_commands_by_testdata_title" >
        <Arguments>
            <argument name="system_name" value="cds" />
            <argument name="title" value="login_ssh" />
            <argument name="description" value="my own description" />
        </Arguments>
        <onError action='next' value='4'  />
        <rmt></rmt>
        <context>positive</context>
        </step>

        <step Driver="cli_driver" Keyword="send_commands_by_testdata_title" >
        <Arguments>
            <argument name="system_name" value="cds" />
            <argument name="title" value="logout_ssh" />
            <argument name="description" value="my own description" />
        </Arguments>
        <onError action='next' value='4'  />
        <rmt></rmt>
            <impact>noimpact</impact>
        <context>positive</context>
        </step>

        <step Driver="cli_driver" Keyword="disconnect" >
        <Arguments>
            <argument name="system_name" value="ne1_tl1" />
            <argument name="description" value="my own description" />
        </Arguments>
        <onError action='next' value='4'  />
        <rmt></rmt>
        <context>positive</context>
        </step>

        <step Driver="cli_driver" Keyword="disconnect" >
        <Arguments>
            <argument name="system_name" value="cds" />
            <argument name="description" value="my own description" />
        </Arguments>
        <onError action='next' value='4'  />
        <rmt></rmt>
        <context>positive</context>
        </step>

    </Steps>

</Testcase>